{
  "name": "phising",
  "version": "0.0.1",
  "description": "",
  "author": "phising",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "start:dev": "nest start auth --watch",
    "start:auth:debug": "nest start auth --debug --watch",
    "start:auth:prod": "node dist/apps/auth/main",
    "build": "nest build auth && nest build campaign && nest build notifications",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "format:check": "prettier --check \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "start:prod": "pm2 start ecosystem.config.js --env production"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/swagger": "^7.1.1",
    "@typegoose/auto-increment": "^4.12.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/nodemailer": "^7.0.0",
    "@types/xlsx": "^0.0.35",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "fast-csv": "^5.0.2",
    "fluent-ffmpeg": "^2.1.3",
    "joi": "^17.9.2",
    "mongodb": "5.7.0",
    "mongoose": "^7.3.4",
    "multer": "1.4.5-lts.1",
    "nestjs-pino": "^3.3.0",
    "nodemailer": "^7.0.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pbkdf2": "^3.1.2",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.8",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.4.2",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-newlines": "^1.3.4",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
